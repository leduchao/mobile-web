@model MobileWeb.Models.Entities.Category

@{
    ViewData["Title"] = "Details";
}

<h1 class="text-dark"">Chi tiết danh mục</h1>
<hr />

<div>
    <dl class="row">
        <dt class="col-sm-2 text-dark">Tên danh mục</dt>
        <dd class="col-sm-10 text-dark">
            @Html.DisplayFor(model => model.Name)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-outline-primary">Chỉnh sửa</a>
    <span class="m-2">|</span>
    <a asp-action="Index">Quay lại</a>
</div>
