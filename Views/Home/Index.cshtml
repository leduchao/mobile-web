@{
	ViewData["Title"] = "Trang chủ";
}

<div id="carouselExampleAutoplaying" class="carousel slide" data-bs-ride="carousel">
	<div class="carousel-inner">
		<div class="carousel-item active">
			<img src="https://s.cafebazaar.ir/images/upload/screenshot/com.muneerhallows.iphonewallpapers-195b4dce-7321-4ed9-8023-691fa3250ca6.webp?x-img=v1/resize,h_600,lossless_false/optimize" class="d-block w-100" height="657" alt="...">
			<div class="carousel-caption d-none d-md-block">
				<h5>Iphone 15 Series</h5>
				<p>Some representative placeholder content for the first slide.</p>
			</div>
		</div>
		<div class="carousel-item">
			<img src="https://www.pixel4k.com/wp-content/uploads/2019/11/oppo-find-x_1574938873.jpg" class="d-block w-100" height=657 alt="...">
			<div class="carousel-caption d-none d-md-block">
				<h5>Oppo Find X Series</h5>
				<p>Some representative placeholder content for the third slide.</p>
			</div>
		</div>
		<div class="carousel-item">
			<img src="https://www.kimstore.com/cdn/shop/articles/S24_Ultra_Banner.png?v=1705634996" class="d-block w-100" height="657" alt="...">
			<div class="carousel-caption d-none d-md-block">
				<h5>Samsung Galaxy S24 Series</h5>
				<p>Some representative placeholder content for the second slide.</p>
			</div>
		</div>
	</div>
	<button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleAutoplaying" data-bs-slide="prev">
		<span class="carousel-control-prev-icon" aria-hidden="true"></span>
		<span class="visually-hidden">Previous</span>
	</button>
	<button class="carousel-control-next" type="button" data-bs-target="#carouselExampleAutoplaying" data-bs-slide="next">
		<span class="carousel-control-next-icon" aria-hidden="true"></span>
		<span class="visually-hidden">Next</span>
	</button>
</div>

@await Component.InvokeAsync("FeatureProducts");